
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        String      @id @default(uuid())
  userName  String      @unique
  name      String?  
  age       Int? 
  email     String   @unique
  password  String
  confirmPassword String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
